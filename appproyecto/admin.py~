from django.contrib import admin
from appproyecto.models import usuario, album, notificacion, comentario, calendario, instagram, soundcloud, youtube, album_instagram, album_soundcloud, album_youtube


admin.site.register(usuario)
admin.site.register(album)
admin.site.register(comentario)
admin.site.register(calendario)
admin.site.register(instagram)
admin.site.register(soundcloud)
admin.site.register(youtube)
admin.site.register(album_instagram)
admin.site.register(album_soundcloud)
admin.site.register(album_youtube)




